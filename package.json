{
  "name": "kosmos-cli",
  "description": "React Static Site Generator",
  "version": "0.1.0",
  "homepage": "https://github.com/velocitycoding/kosmos-cli",
  "keywords": [
    "cli",
    "command line"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "preferGlobal": true,
  "bin": {
    "kosmos": "./bin/kosmos"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "minimist": "~0.0.8",
    "verbalize": "~0.1.1",
    "vorpal": "^1.5.5"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "chai": "3.5.0",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "mocha": "2.4.5",
    "verb": "~0.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
